{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ES2020",
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"outDir": "./", "rootDir": "./",
		"strict": true,
		"esModuleInterop": true, 
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"declaration": false,
		"sourceMap": false,
		"moduleResolution": "node",
		"noEmitOnError": false },
	"include":
		["*.ts"],
	"exclude": ["node_modules"]
}
